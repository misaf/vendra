{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "afsakar/filament-translate-action": "^1.1.0",
        "bezhansalleh/filament-language-switch": "^3.1.0",
        "brick/money": "^0.8.1",
        "filament/filament": "^3.2.93",
        "filament/spatie-laravel-media-library-plugin": "^3.2.93",
        "filament/spatie-laravel-settings-plugin": "^3.2.93",
        "filament/spatie-laravel-tags-plugin": "^3.2.93",
        "filament/spatie-laravel-translatable-plugin": "^3.2.93",
        "flowframe/laravel-trend": "^0.1.5",
        "guzzlehttp/guzzle": "^7.8.1",
        "internachi/modular": "^2.2",
        "laravel-json-api/cursor-pagination": "^3.0",
        "laravel-json-api/hashids": "^2.0",
        "laravel-json-api/laravel": "^3.4",
        "laravel/framework": "^10.48.16",
        "laravel/horizon": "^5.25.0",
        "laravel/pulse": "^1.2.4",
        "laravel/sanctum": "^3.3.3",
        "laravel/telescope": "^5.1.1",
        "laravel/tinker": "^2.9",
        "league/flysystem-sftp-v3": "^3.28.0",
        "mokhosh/filament-jalali": "^3.3.0",
        "owenvoke/blade-fontawesome": "^2.6.0",
        "spatie/eloquent-sortable": "^4.4.0",
        "spatie/laravel-activitylog": "^4.8.0",
        "spatie/laravel-model-status": "^1.18.0",
        "spatie/laravel-multitenancy": "^3.2",
        "spatie/laravel-permission": "^6.9",
        "spatie/laravel-queueable-action": "^2.15.0",
        "spatie/laravel-sluggable": "^3.6",
        "spatie/laravel-translation-loader": "^2.8.0",
        "staudenmeir/belongs-to-through": "^2.15.1",
        "staudenmeir/eloquent-has-many-deep": "^1.19.4",
        "staudenmeir/laravel-adjacency-list": "^1.20.1",
        "termehsoft/blog": ">=1",
        "termehsoft/currency": ">=1",
        "termehsoft/faq": ">=1",
        "termehsoft/geographical": ">=1",
        "termehsoft/language": ">=1",
        "termehsoft/order": ">=1",
        "termehsoft/page": ">=1",
        "termehsoft/permission": ">=1",
        "termehsoft/product": ">=1",
        "termehsoft/tenant": ">=1",
        "termehsoft/transaction": ">=1",
        "termehsoft/user": ">=1",
        "ysfkaya/filament-phone-input": "^2.3.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13.5",
        "fakerphp/faker": "^1.23.1",
        "jasonmccreary/laravel-test-assertions": "^2.4.1",
        "laravel-json-api/testing": "^2.1",
        "laravel/pint": "^1.16.2",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^7.10",
        "pestphp/pest": "^2.34.9",
        "pestphp/pest-plugin-stressless": "^2.2",
        "phpunit/phpunit": "^10.5.17",
        "spatie/laravel-ignition": "^2.8.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "app-modules/*",
            "options": {
                "symlink": true
            }
        }
    ]
}
