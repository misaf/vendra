{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "afsakar/filament-translate-action": "^1.0.2",
        "ashallendesign/laravel-exchange-rates": "^7.4",
        "bezhansalleh/filament-language-switch": "^3.1.0",
        "bezhansalleh/filament-panel-switch": "^1.0.5",
        "brick/money": "^0.8.1",
        "filament/filament": "^3.2.76",
        "filament/spatie-laravel-media-library-plugin": "^3.2.76",
        "filament/spatie-laravel-settings-plugin": "^3.2.76",
        "filament/spatie-laravel-tags-plugin": "^3.2.76",
        "filament/spatie-laravel-translatable-plugin": "^3.2.76",
        "flowframe/laravel-trend": "^0.1.5",
        "guzzlehttp/guzzle": "^7.8.1",
        "laravel-json-api/cursor-pagination": "^3.0",
        "laravel-json-api/hashids": "^2.0",
        "laravel-json-api/laravel": "^3.4",
        "laravel/framework": "^10.48.10",
        "laravel/horizon": "^5.24.4",
        "laravel/pulse": "^1.1",
        "laravel/sanctum": "^3.3.3",
        "laravel/telescope": "^5.0",
        "laravel/tinker": "^2.9",
        "laravel/vonage-notification-channel": "^3.3",
        "league/flysystem-sftp-v3": "^3.26.0",
        "mcamara/laravel-localization": "^2.0.1",
        "mokhosh/filament-jalali": "^3.2.8",
        "morilog/jalali": "^3.4.2",
        "owenvoke/blade-fontawesome": "^2.6.0",
        "pelmered/filament-money-field": "^1.3.0",
        "pusher/pusher-php-server": "^7.2.4",
        "spatie/eloquent-sortable": "^4.3.0",
        "spatie/laravel-activitylog": "^4.8.0",
        "spatie/laravel-honeypot": "^4.5.2",
        "spatie/laravel-model-status": "^1.17.2",
        "spatie/laravel-permission": "^6.7",
        "spatie/laravel-queueable-action": "^2.15.0",
        "spatie/laravel-sluggable": "^3.6",
        "spatie/laravel-translation-loader": "^2.8.0",
        "staudenmeir/belongs-to-through": "^2.15.1",
        "staudenmeir/eloquent-has-many-deep": "^1.19.3",
        "staudenmeir/laravel-adjacency-list": "^1.20.1",
        "ysfkaya/filament-phone-input": "^2.3.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13.5",
        "fakerphp/faker": "^1.23.1",
        "jasonmccreary/laravel-test-assertions": "^2.4",
        "laravel-json-api/testing": "^2.1",
        "laravel/pint": "^1.15.3",
        "laravel/sail": "^1.29.1",
        "mockery/mockery": "^1.6.11",
        "nunomaduro/collision": "^7.10",
        "pestphp/pest": "^2.34.7",
        "pestphp/pest-plugin-stressless": "^2.2",
        "phpunit/phpunit": "^10.5.17",
        "spatie/laravel-ignition": "^2.7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
